    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-katex": "^5.0.0",
    "remark-gfm": "^1.0.0",
    "remark-math": "^4.0.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "timeago-react": "^3.0.2",
    "typescript": "^4.3.2"
  }
